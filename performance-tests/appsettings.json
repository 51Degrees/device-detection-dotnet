{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  // For a sample configuration file demonstrating all available options, see 
  // https://github.com/51Degrees/device-detection-dotnet/blob/master/Examples/sample-configuration.json
  "PipelineOptions": {
    "Elements": [
      {
        "BuilderName": "DeviceDetectionHashEngineBuilder",
        "BuildParameters": {
          "DataFile": "../FiftyOne.DeviceDetection/device-detection-cxx/device-detection-data/51Degrees-LiteV4.1.hash",
          "CreateTempDataCopy": false,
          "AutoUpdate": false,
          "PerformanceProfile": "HighPerformance",
          "DataFileSystemWatcher": false,
          "DataUpdateOnStartUp": false
        }
      }
    ]
  }
}
