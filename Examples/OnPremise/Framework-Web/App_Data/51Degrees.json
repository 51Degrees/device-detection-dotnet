{
  // For a sample configuration file demonstrating all available options, see 
  // https://github.com/51Degrees/device-detection-dotnet/blob/master/Examples/sample-configuration.json
  "PipelineOptions": {
    "Elements": [
      {
        "BuilderName": "DeviceDetectionHashEngineBuilder",
        "BuildParameters": {
          "DataFile": "C:\\PATH\\TO\\51Degrees-LiteV4.1.hash",
          "CreateTempDataCopy": false,
          "AutoUpdate": false,
          "PerformanceProfile": "LowMemory",
          "DataFileSystemWatcher": false,
          "DataUpdateOnStartUp": false
        }
      },
      {
        "BuilderName": "JavaScriptBuilderElement",
        "BuildParameters": {
          "Minify": true
        }
      }
    ],
    "BuildParameters": {
      "ClientSideEnabled": true
    }
  }
}
