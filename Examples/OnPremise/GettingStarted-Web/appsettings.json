{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "PipelineOptions": {
    "Elements": [
      {
        "BuilderName": "DeviceDetectionHashEngineBuilder",
        "BuildParameters": {
          "DataFile": "51Degrees-LiteV4.1.hash",
          "CreateTempDataCopy": false,
          "AutoUpdate": false,
          "PerformanceProfile": "LowMemory",
          "DataFileSystemWatcher": false,
          "DataUpdateOnStartUp": false
        }
      },
      {
        "BuilderName": "JavaScriptBuilderElement",
        "BuildParameters": {
          "Minify": true
        }
      }
    ],
    "PipelineBuilderParameters": {
      // Set this to true in production in order to share your usage data with 51Degrees. This is 
      // important to maintain accurate detections in the changing landscape of connected devices 
      // and software.
      "ShareUsage": false
    }
  },
  "PipelineWebIntegrationOptions": {
    // Both these options default to true anyway.
    // They are specified here for illustrative purposes.
    "ClientSideEvidenceEnabled": true,
    "UseAsyncScript": true
  }
}
